<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ControlViews="clr-namespace:ElibWpf.Views.Controls"
                    xmlns:ControlViewModels="clr-namespace:ElibWpf.ViewModels.Controls"
                    xmlns:WindowViews="clr-namespace:ElibWpf.Views.Windows"
                    xmlns:WindowViewModels="clr-namespace:ElibWpf.ViewModels.Windows">
    <DataTemplate DataType="{x:Type ControlViewModels:DashboardViewModel}">
        <ControlViews:DashboardControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ControlViewModels:BooksViewModel}">
        <ControlViews:BooksControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ControlViewModels:QuotesViewModel}">
        <ControlViews:QuotesControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ControlViewModels:SettingsViewModel}">
        <ControlViews:SettingsControl />
    </DataTemplate>
</ResourceDictionary>