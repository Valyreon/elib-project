<Application
    x:Class="Valyreon.Elib.Wpf.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d1p1:Ignorable="d"
    Exit="OnExit"
    Startup="OnStartup"
    StartupUri="Views/Windows/TheWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Colors.xaml" />
                <ResourceDictionary Source="Resources/ScrollBarStyles.xaml" />
                <ResourceDictionary Source="Resources/DashboardTabControlStyles.xaml" />
                <ResourceDictionary Source="Resources/BookTileSubLinksStyle.xaml" />
                <ResourceDictionary Source="Resources/IconDrawings.xaml" />
                <ResourceDictionary Source="Resources/Miscellaneous.xaml" />
                <ResourceDictionary Source="Resources/DataGridStyles.xaml" />
                <ResourceDictionary Source="Resources/ComboBoxStyles.xaml" />
                <ResourceDictionary Source="Resources/WindowStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
